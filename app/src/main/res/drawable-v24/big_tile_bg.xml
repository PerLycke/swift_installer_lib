<?xml version="1.0" encoding="utf-8"?>
<ripple android:color="?android:colorControlHighlight" xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@+id/background">
        <shape android:shape="rectangle">
            <corners android:radius="4dp" />
        </shape>
    </item>
    <item android:id="@+id/stroke">
        <shape android:shape="rectangle">
            <corners android:radius="4dp" />
            <stroke android:color="#14ffffff" android:width="1dp" />
        </shape>
    </item>
</ripple>