<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@+id/dialog_bg">
        <inset android:insetLeft="18.0dip" android:insetRight="18.0dip">
        <shape android:shape="rectangle">
            <corners android:radius="4dp" />
        </shape>
        </inset>
    </item>
    <item android:id="@+id/dialog_stroke">
        <shape android:shape="rectangle">
            <corners android:radius="4dp" />
            <stroke android:color="@color/dialog_stroke" android:width="1dp" />
        </shape>
    </item>
</layer-list>

