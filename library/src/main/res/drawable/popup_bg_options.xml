<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:id="@+id/background_popup"
        android:bottom="0.0dip"
        android:left="0.0dip"
        android:right="0.0dip"
        android:top="0.0dip">
        <shape android:shape="rectangle">
            <corners android:radius="4dp" />
        </shape>
    </item>
    <item
        android:id="@+id/stroke_popup"
        android:bottom="0.0dip"
        android:left="0.0dip"
        android:right="0.0dip"
        android:top="0.0dip">
        <shape android:shape="rectangle">
            <corners android:radius="4dp" />
            <stroke
                android:width="1dp"
                android:color="#14ffffff" />
        </shape>
    </item>
</layer-list>
