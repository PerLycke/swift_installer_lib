<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/customize_preview_root"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/preview_nav_layout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/settings_guideline1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_begin="201dp" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/settings_guideline2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            app:layout_constraintGuide_begin="253dp" />

        <ImageView
            android:id="@+id/nav_preview"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:contentDescription="@string/image"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0"
            app:srcCompat="@drawable/preview_nav_frame_bg" />

        <ImageView
            android:id="@+id/contact_tile_icon_1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="12dp"
            android:contentDescription="@string/image"
            android:maxWidth="8dp"
            android:maxHeight="8dp"
            app:layout_constraintBottom_toBottomOf="@+id/contact_tile_name_1"
            app:layout_constraintEnd_toStartOf="@+id/contact_tile_name_1"
            app:layout_constraintTop_toTopOf="@+id/contact_tile_name_1"
            app:srcCompat="@drawable/contact_tile_icon_1_pastel" />

        <TextView
            android:id="@+id/contact_tile_name_1"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="80dp"
            android:layout_marginTop="30dp"
            android:layout_marginEnd="8dp"
            android:text="@string/contact_name_1"
            android:textColor="@android:color/white"
            android:textSize="12sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_chainStyle="packed" />

        <ImageView
            android:id="@+id/contact_tile_icon_2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="12dp"
            android:contentDescription="@string/image"
            android:maxWidth="8dp"
            android:maxHeight="8dp"
            app:layout_constraintBottom_toBottomOf="@+id/contact_tile_name_2"
            app:layout_constraintEnd_toStartOf="@+id/contact_tile_name_2"
            app:layout_constraintTop_toTopOf="@+id/contact_tile_name_2"
            app:srcCompat="@drawable/contact_tile_icon_1_pastel" />

        <TextView
            android:id="@+id/contact_tile_name_2"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="80dp"
            android:layout_marginTop="30dp"
            android:layout_marginEnd="8dp"
            android:text="@string/contact_name_2"
            android:textColor="@android:color/white"
            android:textSize="12sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="@+id/nav_preview"
            app:layout_constraintTop_toBottomOf="@+id/contact_tile_name_1" />

        <ImageView
            android:id="@+id/contact_tile_icon_3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="12dp"
            android:contentDescription="@string/image"
            android:maxWidth="8dp"
            android:maxHeight="8dp"
            app:layout_constraintBottom_toBottomOf="@+id/contact_tile_name_3"
            app:layout_constraintEnd_toStartOf="@+id/contact_tile_name_3"
            app:layout_constraintTop_toTopOf="@+id/contact_tile_name_3"
            app:srcCompat="@drawable/contact_tile_icon_1_pastel" />

        <TextView
            android:id="@+id/contact_tile_name_3"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="80dp"
            android:layout_marginTop="30dp"
            android:layout_marginEnd="8dp"
            android:text="@string/contact_name_3"
            android:textColor="@android:color/white"
            android:textSize="12sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="@+id/nav_preview"
            app:layout_constraintTop_toBottomOf="@+id/contact_tile_name_2" />

        <ImageView
            android:id="@+id/navbar_bg"
            android:layout_width="0dp"
            android:layout_height="24dp"
            android:layout_marginTop="28dp"
            android:layout_marginBottom="3dp"
            android:layout_marginStart="3dp"
            android:layout_marginEnd="3dp"
            android:contentDescription="@string/image"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:srcCompat="@drawable/preview_nav_bg" />

        <ImageView
            android:id="@+id/nav_back_icon"
            android:layout_width="16dp"
            android:layout_height="16dp"
            android:layout_marginStart="40dp"
            android:contentDescription="@string/image"
            app:layout_constraintBottom_toBottomOf="@+id/navbar_bg"
            app:layout_constraintStart_toStartOf="@+id/navbar_bg"
            app:layout_constraintTop_toTopOf="@+id/navbar_bg"
            app:srcCompat="@drawable/nav_back_icon_stock" />

        <ImageView
            android:id="@+id/nav_home_icon"
            android:layout_width="16dp"
            android:layout_height="16dp"
            android:layout_marginStart="76dp"
            android:contentDescription="@string/image"
            app:layout_constraintBottom_toBottomOf="@+id/navbar_bg"
            app:layout_constraintStart_toStartOf="@+id/nav_back_icon"
            app:layout_constraintTop_toTopOf="@+id/navbar_bg"
            app:srcCompat="@drawable/nav_home_icon_stock" />

        <ImageView
            android:id="@+id/nav_recent_icon"
            android:layout_width="16dp"
            android:layout_height="16dp"
            android:layout_marginStart="72dp"
            android:layout_marginEnd="40dp"
            android:contentDescription="@string/image"
            app:layout_constraintBottom_toBottomOf="@+id/navbar_bg"
            app:layout_constraintEnd_toEndOf="@+id/navbar_bg"
            app:layout_constraintTop_toTopOf="@+id/navbar_bg"
            app:srcCompat="@drawable/nav_recent_icon_stock" />

        </androidx.constraintlayout.widget.ConstraintLayout>


    </androidx.constraintlayout.widget.ConstraintLayout>
